function ped_set_cookie(e, o, t) {
    var i = new Date, n = new Date; (null == t || 0 == t) && (t = 1), n.setTime(i.getTime() + 864e5 * t);
    var c = e + '=' + escape(o) + '; expires=' + n.toGMTString() + '; path=/'; document.cookie = c;
}

function ped_get_cookie(cname) {
    var ret = "";
    var name = cname + '=';
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
    }
    return ret;
}
