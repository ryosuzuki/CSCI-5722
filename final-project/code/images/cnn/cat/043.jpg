function checkgoogletagapi() {
    if (typeof(googletag) == "undefined") {
        delayedcheckpageads();  // will display zero ads
    }
    else {
        // wait till googletag is ready, to validate ads that are displaying
        if (isset(googletag.apiReady) && googletag.apiReady && function_exists(googletag.pubads) ) {
            googletagready = true;
            delayedcheckpageads();
        }
        else {
            clearTimeout(googletagtimer);
            googletagtimer = setTimeout(function() {checkgoogletagapi();},100);
        }
    }
}

function delayedcheckpageads() {
    clearTimeout(adchecktimer);
    adchecktimer = setTimeout(function() {checkpageads();},500);
}


function checkpageads() {
    var msg;
    var errclass = "";
    var errmsg = "";

    pageadcount = $(pageadselector).size();
    visibleadcount = $(pageadselector+":visible").size();
    checkpageadsinit = true;

    if (pageadcount === 0) {
        msg = "NO ADS FOUND ON PAGE";

        if (typeof(googletag) != "undefined" && typeof(googletag.cmd) != "undefined" && typeof(googletag.cmd.j) != "undefined") {

            if (googletag.cmd.j > 0) {
                errtitle = "AD CODE EXISTS BUT NO ADS ARE RENDERING";
                errclass = "adminerr";
                errmsg = "Ad display code is trying to display ads, but the html display elements are missing. May just need to remove extraneous code.";
            }
        }
    }
    else {
        msg = "# ADS ON PAGE: <em>"+pageadcount+"</em>";

        if (pageadcount != visibleadcount) {
            //msg += " &nbsp; &nbsp; ERROR: Only " + visibleadcount + " Ad(s) are visible";
            errtitle = "AD DISPLAY ERROR";
            errclass = "adminerr";
            errmsg = "Only " + visibleadcount + " Ad(s) are visible, there should be "+ pageadcount +" Ads";

            if (typeof(sponsoredlistingpage) != "undefined" && sponsoredlistingpage) {
            	errtitle = "SPONSORED LISTING PAGE";
            	errclass = "noticemsg";
            	errmsg = "Only " + visibleadcount + " Ad(s) are provided by the ad server. There may be up to "+ pageadcount +" Ads on this page";

            }
            // TRACK ERROR WITH GOOGLE ANALYTICS
            // _trackEvent(category, action, opt_label, opt_value, opt_noninteraction)
            //  _gaq.push(['_trackEvent', 'Benchmarking', 'LoadTimeMillisecs', document.URL, value,true]);
/*          var trackcat, trackact, tracklab, trackval,tracknon;
            trackcat = "SiteError";
