            jQuery(this).find(".arrows").stop(true, true).fadeIn(300);
        },
        function() {
            jQuery(this).find(".arrows").fadeOut(300);
        });
    /* 轮播图 */
    jQuery(".slider").slide({
        titCell: ".hd ul",
        mainCell: ".bd ul",
        autoPage: true,
        effect: "leftLoop",
        autoPlay: true,
        vis: 3,
        trigger: "mouseover",
        pnLoop:true,
    });

    </script>
</body>

</html>
